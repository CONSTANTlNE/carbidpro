<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;

class CarPayment extends Model
{
    use HasFactory;

    protected $fillable = [
        'car_id',
        'all_cost',
        'left_days',
        'financed_fee',
        'cost_with_extra',
        'payment_date',
    ];

   
}
